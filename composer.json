{
    "name": "michelangelomo/easyfilter",
    "description": "EasyFilter helps you to filter eloquent queries",
    "type": "library",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Michelangelo",
            "email": "michelangelo@morrillo.it"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "extra": {
